          crossorigin="anonymous">
    <link href="~/Content/Style.css" rel="stylesheet" />
    <link href="~/Content/mobile-style.css" rel="stylesheet" />
</head>
<body style="background-image:url('../Hinh/anh2.jpg')">

    @RenderBody()
